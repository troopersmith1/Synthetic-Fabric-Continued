<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>CreateSynthread</defName>
		<label>Make Synthread</label>
		<description>Make Synthread from Devilstrand, cloth and steel.</description>
		<researchPrerequisite>SynthreadTechCrafting</researchPrerequisite>
		<jobString>Making Synthread</jobString>
		<workAmount>175</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.75</workSkillLearnFactor>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>DevilstrandCloth</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>45</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>DevilstrandCloth</li>
				<li>Cloth</li>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Synthread>75</Synthread>
		</products>
		<skillRequirements>
				<Crafting>6</Crafting>
		</skillRequirements>
	</RecipeDef>

	<RecipeDef>
		<defName>CreateHyperweave</defName>
		<label>Make Hyperweave</label>
		<description>Make Hyperweave from Synthread and plasteel.</description>
		<researchPrerequisite>HyperweaveTechCrafting</researchPrerequisite>
		<jobString>Making Hyperweave</jobString>
		<workAmount>325</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>1.00</workSkillLearnFactor>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>1</targetCountAdjustment>
		<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Synthread</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>	
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Synthread</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Hyperweave>75</Hyperweave>
		</products>
		<skillRequirements>
				<Crafting>12</Crafting>
		</skillRequirements>
	</RecipeDef>


</Defs>